    Name = "Enable Anti-Ban System",
    Default = false,
    Callback = function(value)
        AntiBanEnabled = value
        if value then
            EnableAntiBan()
            print("Anti-Ban: Enabled")
        else
            DisableAntiBan()
            print("Anti-Ban: Disabled")
        end
    end
})

